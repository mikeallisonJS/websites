{
  "name": "offer.captainssounds.com",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/offer.captainssounds.com/src",
  "prefix": "websites",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/offer.captainssounds.com/dist",
        "index": "packages/offer.captainssounds.com/src/index.html",
        "main": "packages/offer.captainssounds.com/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "packages/offer.captainssounds.com/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "packages/offer.captainssounds.com/src/favicon.ico",
          "packages/offer.captainssounds.com/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/purple-green.css",
          "packages/offer.captainssounds.com/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "packages/offer.captainssounds.com/src/environments/environment.ts",
              "with": "packages/offer.captainssounds.com/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve-angular": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "offer.captainssounds.com:build:production"
        },
        "development": {
          "buildTarget": "offer.captainssounds.com:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "nx serve-angular offer.captainssounds.com --configuration=production"
          },
          {
            "command": "bash packages/offer.captainssounds.com/emulator.sh"
          }
        ],
        "parallel": true
      }
    },
    "deploy-functions": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npm run build && npm run deploy"
          }
        ],
        "cwd": "packages/offer.captainssounds.com/functions"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "offer.captainssounds.com:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/offer.captainssounds.com/jest.config.ts"
      }
    }
  },
  "tags": []
}
