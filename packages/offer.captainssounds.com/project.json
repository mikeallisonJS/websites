{
  "name": "offer.captainssounds.com",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/offer.captainssounds.com/src",
  "prefix": "websites",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/offer.captainssounds.com/dist",
        "index": "packages/offer.captainssounds.com/src/index.html",
        "main": "packages/offer.captainssounds.com/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "packages/offer.captainssounds.com/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "packages/offer.captainssounds.com/src/favicon.ico",
          "packages/offer.captainssounds.com/src/assets"
        ],
        "styles": [
          "@angular/material/prebuilt-themes/purple-green.css",
          "packages/offer.captainssounds.com/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "offer.captainssounds.com:build:production"
        },
        "development": {
          "browserTarget": "offer.captainssounds.com:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "offer.captainssounds.com:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/offer.captainssounds.com/**/*.ts",
          "packages/offer.captainssounds.com/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/offer.captainssounds.com/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      },
      "fetch-secrets": {
        "executor": "nx:run-commands",
        "options": {
          "commands": [
            {
              "command": "doppler secrets download --no-file --format=env-no-quotes -p members-captainssounds-com > packages/members.captainssounds.com/.env"
            }
          ]
        }
    }
  },
  "tags": []
}
