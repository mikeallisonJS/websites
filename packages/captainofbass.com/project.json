{
  "name": "captainofbass.com",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/captainofbass.com/src",
  "prefix": "websites",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/captainofbass.com/dist",
        "index": "packages/captainofbass.com/src/index.html",
        "main": "packages/captainofbass.com/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "packages/captainofbass.com/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "packages/captainofbass.com/src/favicon.ico",
          "packages/captainofbass.com/src/assets"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "packages/captainofbass.com/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1500kb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "packages/captainofbass.com/src/environments/environment.ts",
              "with": "packages/captainofbass.com/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "captainofbass.com:build:production"
        },
        "development": {
          "buildTarget": "captainofbass.com:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "captainofbass.com:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/captainofbass.com/jest.config.ts"
      }
    },
    "fetch-secrets": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "doppler secrets download --no-file --format=env-no-quotes -p captainofbass-com > packages/captainofbass.com/.env"
          }
        ]
      }
    }
  },
  "tags": []
}
